@model DTO.PostDTO

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Post Detail</title>
</head>
<body>
<div>
    <div>
        <h3>Post: @Model.Title</h3>
        <p>Content: @Model.ShortContent</p>
        <p>Link: @Model.SeoLink</p>
        <p>Category: @Model.CategoryName</p>
        <button>
            <a href="/Admin/Post/PostList">
                All Posts
            </a>
        </button>
    </div>
</div>
</body>
</html>